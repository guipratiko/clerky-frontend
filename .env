# Configurações do Frontend
PORT=3500
GENERATE_SOURCEMAP=false

# URLs do Backend - Produção
REACT_APP_API_URL=https://back.clerky.com.br
REACT_APP_SOCKET_URL=https://back.clerky.com.br
REACT_APP_UPLOADS_URL=https://back.clerky.com.br/uploads

# URLs do Frontend (para documentação)
REACT_APP_FRONTEND_URL=https://app.clerky.com.br

# Configurações de Ambiente
NODE_ENV=production

# Configurar Hot Reload para produção
FAST_REFRESH=true
DANGEROUSLY_DISABLE_HOST_CHECK=true

# Configurações específicas do webpack-dev-server
WDS_SOCKET_HOST=app.clerky.com.br
WDS_SOCKET_PORT=443
WDS_SOCKET_PATH=/ws
WDS_SOCKET_PROTOCOL=wss